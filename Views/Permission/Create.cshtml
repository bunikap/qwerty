@model qwerty.Models.Permission

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Permission</h4>
<hr />
<form asp-action="Create">
    <div class="row">
        <div class="col-md-4">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="permission" class="control-label"></label>
                <input asp-for="permission" class="form-control" />
                <span asp-validation-for="permission" class="text-danger"></span>
            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group lp-4">
                <input type="button" class="btn btn-primary t-5" value="Back"
                    onclick="location.href='@Url.Action("Index", "UserPer")'" />
                <input type="submit" value="Create" class="btn btn-primary" />
                 <input type="button" class="btn btn-primary t-5" value="Delete"
                    onclick="location.href='@Url.Action("Index", "Permission")'" />
            </div>
        </div>
    </div>
</form>
</div>
</div>



@section Scripts {
@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
